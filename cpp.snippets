############ Ace related ##############
snippet trc
	ACE_TRACE ("${1:your text}");

snippet aa
	#include "ace/${1:your desired ACE header file}"

snippet adef1
	#include "ace/OS_NS_string.h"
	#include "ace/Log_Msg.h"
	#include "ace/SString.h"
	
	${1}

snippet adef2
	#include "ace/Log_Msg.h"
	#include "ace/INET_Addr.h"
	#include "ace/SOCK_Connector.h"
	#include "ace/SOCK_Stream.h"
	#include "ace/SOCK_Acceptor.h"

snippet cchst
	const ACE_TCHAR *text = ACE_TEXT("${1}") ;

snippet tmain
	int ACE_TMAIN (${1})
	{
		${2}
		return 0 ;
	}

snippet ntrace0
	#if defined (ACE_NTRACE)
	#undef ACE_NTRACE
	#endif
	#define ACE_NTRACE 0

snippet atx 
	ACE_TEXT ("${1:Write your text here}")${2}

snippet adb1
	ACE_DEBUG ((LM_DEBUG,
					  	ACE_TEXT("[%T] ${1:your text here..}")${2}
					    ));

snippet adb2
	ACE_DEBUG ((LM_DEBUG,
						  ACE_TEXT("[%T] ${1:Write your text here}")${2}));

snippet adb3
	ACE_DEBUG ((LM_DEBUG, ACE_TEXT("[%T] ${1:Write your text here}")${2}));

snippet adb4
	ACE_DEBUG ((LM_DEBUG, "${1:Write your text here}"${2}));

snippet adb5
	ACE_DEBUG ((LM_DEBUG, 
	            "${1:Write your text here}"${2}));

snippet aer1
	ACE_ERROR ((LM_ERROR,
	            ACE_TEXT("${1:your text..}")${2:}));

snippet aern1
	ACE_ERROR_RETURN ((LM_ERROR,
										 "[%T] ${1:your text here..}"),
										 ${2:}),${3:your_return_value});

